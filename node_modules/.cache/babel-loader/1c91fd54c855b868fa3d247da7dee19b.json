A2BA;AACA,SAAA,aAAA,GAAA,IAAA,CAAA,KAAA,CAAA,KAAA,IAAA,CAAA,IAAA,EAAA,SAAA,IAAA,EAAA;AACA,SAAA,QAAA,GAAA,IAAA,CAAA,KAAA,CAAA,KAAA,IAAA,CAAA,IAAA,EAAA,MAAA,IAAA,EAAA;AACA,SAAA,UAAA,GAAA,IAAA,CAAA,KAAA,CAAA,KAAA,IAAA,CAAA,IAAA,EAAA,SAAA,IAAA,EAAA;AACA,SAAA,aAAA,GAAA,IAAA,CAAA,KAAA,CAAA,KAAA,IAAA,CAAA,IAAA,EAAA,QAAA,IAAA,EAAA;AACA,SAAA,UAAA,GAAA,IAAA,CAAA,KAAA,CAAA,KAAA,IAAA,CAAA,IAAA,EAAA,KAAA,IAAA,EAAA;AACA,SAAA,KAAA,GAAA,KAAA,IAAA,CAAA,GAAA,IAAA,EAAA;AACA,GAlCA;AAmCA,EAAA,QAAA,oBACA,QAAA,CAAA,CAAA,MAAA,CAAA,CADA,CAnCA;AAsCA,EAAA,OAAA,EAAA;AACA,IAAA,kBADA,8BACA,IADA,EACA;AAAA;;AACA,WAAA,kBAAA,GAAA,GAAA,CAAA,eAAA,CAAA,IAAA,CAAA,GAAA,CAAA;AACA,WAAA,MAAA,CAAA,MAAA,CAAA,QAAA,EAAA,IAAA,CAAA,GAAA;AACA,UAAA,MAAA,GAAA,IAAA,UAAA,EAAA;AACA,MAAA,MAAA,CAAA,aAAA,CAAA,IAAA,CAAA,GAAA;;AACA,MAAA,MAAA,CAAA,MAAA,GAAA,YAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,qBAAA,MAAA,CAAA,MAAA;AACA,QAAA,KAAA,CAAA,WAAA,GAAA,MAAA,CAAA,MAAA;AACA,OAHA;;AAIA,MAAA,MAAA,CAAA,OAAA,GAAA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,SAAA,EAAA,KAAA;AACA,OAFA;AAGA,KAbA;AAcA;AACA,IAAA,YAfA,0BAeA;AACA,UAAA,IAAA,GAAA,IAAA;AACA,MAAA,IAAA,CAAA,MAAA,CAAA,MAAA,CAAA,WAAA,EAAA,KAAA,aAAA;AACA,MAAA,IAAA,CAAA,MAAA,CAAA,MAAA,CAAA,KAAA,EAAA,KAAA,QAAA;;AACA,UAAA,KAAA,aAAA,KAAA,EAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA,aAAA;AACA,QAAA,IAAA,CAAA,MAAA,CAAA,MAAA,CAAA,UAAA,EAAA,KAAA,aAAA;AACA;;AACA,MAAA,IAAA,CAAA,MAAA,CAAA,IAAA,CAAA,UAAA,cAAA,EAAA,IAAA,CAAA,MAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,MAAA,KAAA,GAAA,EAAA;AACA,UAAA,IAAA,CAAA,KAAA,GAAA,IAAA,CAAA,WAAA;AACA,UAAA,IAAA,CAAA,MAAA,CAAA,MAAA,CAAA,SAAA,EAAA,IAAA,CAAA,KAAA;AACA,UAAA,IAAA,CAAA,MAAA,GAAA,IAAA,QAAA,EAAA;AACA;AACA,OANA;AAOA;AA9BA;AAtCA,CAAA","sourcesContent":["<template>\r\n  <div class=\"user-detail\">\r\n    <div class=\"user-detail-top\">基本信息</div>\r\n    <div class=\"user-detail-group\">\r\n      <div class=\"user-icon\">\r\n        <span>头像</span>\r\n        <el-upload\r\n          class=\"avatar-uploader\"\r\n          action=\"\"\r\n          :auto-upload=\"false\"\r\n          :show-file-list=\"false\"\r\n          accept=\"image/jpg,image/png\"\r\n          :on-change=\"handleAvatarChange\"\r\n        >\r\n          <img v-if=\"user_avatar_imgUrl\" width=\"100px\" height=\"100px\" :src=\"user_avatar_imgUrl\">\r\n          <img v-else-if=\"image\" width=\"100px\" height=\"100px\" :src=\"image\" >\r\n          <img v-else src=\"../../assets/logo.png\">\r\n          <i class=\"el-icon-plus avatar-uploader-icon\"></i>\r\n        </el-upload>\r\n      </div>\r\n      <div class=\"user-item\">\r\n        <span>手机</span>\r\n        <span>{{ user_phone || \"暂未设置\" }}</span>\r\n      </div>\r\n      <div class=\"user-item\">\r\n        <span>邮箱</span>\r\n        <span>{{ user_email|| \"暂未设置\" }}</span>\r\n      </div>\r\n      <div class=\"user-item\">\r\n        <span>昵称</span>\r\n        <el-input\r\n          type=\"text\"\r\n          placeholder=\"请输入内容\"\r\n          v-model=\"user_nickname\"\r\n          maxlength=\"15\"\r\n          show-word-limit\r\n          clearable\r\n          style=\"width:200px\"\r\n        >\r\n        </el-input>\r\n      </div>\r\n      <div class=\"user-item\">\r\n        <span>性别</span>\r\n        <el-select v-model=\"user_sex\" placeholder=\"请选择\">\r\n          <el-option\r\n            v-for=\"item in options\"\r\n            :key=\"item.value\"\r\n            :label=\"item.label\"\r\n            :value=\"item.value\">\r\n          </el-option>\r\n        </el-select>\r\n      </div>\r\n      <div class=\"user-item\">\r\n        <span>生日</span>\r\n        <el-date-picker\r\n          v-model=\"user_birthday\"\r\n          type=\"date\"\r\n          placeholder=\"选择日期\"\r\n          value-format=\"yyyy-MM-dd\"\r\n          :picker-options=\"allowedDate\">\r\n        </el-date-picker>\r\n      </div>\r\n      <el-button type=\"danger\" @click=\"saveUserInfo()\">修改</el-button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {mapState} from 'vuex'\r\n\r\nexport default {\r\n  data () {\r\n    return {\r\n      user_nickname: '',\r\n      user_sex: '',\r\n      user_phone: '',\r\n      user_birthday: '',\r\n      user_email: '',\r\n      user_avatar_imgUrl: '',\r\n      user_avatar: null,\r\n      file: null,\r\n      image: '',\r\n      upload: new FormData(),\r\n      options: [{\r\n        value: '男',\r\n        label: '男'\r\n      }, {\r\n        value: '女',\r\n        label: '女'\r\n      }],\r\n      allowedDate: {\r\n        disabledDate (time) {\r\n          return time.getTime() > Date.now() - 8.64e6\r\n        }\r\n      }\r\n    }\r\n  },\r\n  mounted () {\r\n    this.user_nickname = JSON.parse(this.user.user).nick_name || ''\r\n    this.user_sex = JSON.parse(this.user.user).gender || ''\r\n    this.user_phone = JSON.parse(this.user.user).telephone || ''\r\n    this.user_birthday = JSON.parse(this.user.user).birthday || ''\r\n    this.user_email = JSON.parse(this.user.user).email || ''\r\n    this.image = this.user.img || ''\r\n  },\r\n  computed: {\r\n    ...mapState(['user'])\r\n  },\r\n  methods: {\r\n    handleAvatarChange (file) {\r\n      this.user_avatar_imgUrl = URL.createObjectURL(file.raw)\r\n      this.upload.append('avatar', file.raw)\r\n      var reader = new FileReader()\r\n      reader.readAsDataURL(file.raw)\r\n      reader.onload = () => {\r\n        console.log('file 转 base64结果：' + reader.result)\r\n        this.user_avatar = reader.result\r\n      }\r\n      reader.onerror = function (error) {\r\n        console.log('Error: ', error)\r\n      }\r\n    },\r\n    // 修改用户信息\r\n    saveUserInfo () {\r\n      const that = this\r\n      that.upload.append('nick_name', this.user_nickname)\r\n      that.upload.append('sex', this.user_sex)\r\n      if (this.user_birthday !== '') {\r\n        console.log(this.user_birthday)\r\n        that.upload.append('birthday', this.user_birthday)\r\n      }\r\n      that.$axios.post('/root'+'/user/update', that.upload).then(function (res) {\r\n        if (res.status === 200) {\r\n          that.image = that.user_avatar\r\n          that.$store.commit('SET_IMG', that.image)\r\n          that.upload = new FormData\r\n        }\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.user-detail{\r\n  width: 70%;\r\n  height: 100%;\r\n  margin: 20px auto;\r\n}\r\n.user-detail-top{\r\n  width: 100%;\r\n  height: 60px;\r\n  line-height: 60px;\r\n  padding-left: 10px;\r\n  font-weight: bold;\r\n}\r\n.user-detail-group .user-icon{\r\n  height: 150px;\r\n  padding: 0 10px;\r\n  background-color: #fff;\r\n  border-bottom: 1px solid #e0e0e0;\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n}\r\n.user-detail-group .user-item{\r\n  height: 50px;\r\n  padding: 0 10px;\r\n  background-color: #fff;\r\n  border-bottom: 1px solid #e0e0e0;\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n}\r\n.user-detail-group input{\r\n  border: 1px solid #ccc;\r\n  outline: none;\r\n  text-align: right;\r\n  width: 200px;\r\n}\r\n.user-detail-group button{\r\n  margin: 60px 0;\r\n  width: 15%;\r\n  font-size: 16px;\r\n  border-radius: 10px;\r\n  border: none;\r\n  outline: none;\r\n}\r\n.right-title-color{\r\n  color: #999;\r\n  font-size: 14px;\r\n}\r\n.avatar-uploader{\r\n  position: relative;\r\n  width: 100px;\r\n  height: 100px;\r\n}\r\n.avatar-uploader /deep/ .el-upload.el-upload--text {\r\n  width: 100%;\r\n  height: 100%;\r\n  border-radius: 50%;\r\n  cursor: pointer;\r\n  overflow: hidden;\r\n}\r\n.avatar-uploader-icon {\r\n  position: absolute;\r\n  left: 0;\r\n  top: 20px;\r\n  font-size: 28px;\r\n  color: #8c939d;\r\n  width: 100%;;\r\n  height: 100%;\r\n  line-height: 56px;\r\n  text-align: center;\r\n  opacity: 0;\r\n}\r\n.avatar-uploader-icon:hover{\r\n  opacity: 1;\r\n}\r\n.avatar {\r\n  position: absolute;\r\n  left: 0;\r\n  top: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  border-radius: 50%;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/children"}]}
HA,CAHA;;AAAA;AACA,gBAAA,CAAA,EADA;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAUA,KA1DA;AA2DA,IAAA,OA3DA,qBA2DA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,sBACA,MAAA,CAAA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,IAAA,IAAA,IADA;AAAA;AAAA;AAAA;;AAEA,gBAAA,MAAA,CAAA,QAAA,CAAA,iBAAA,EAAA,IAAA,EAAA;AACA,kBAAA,iBAAA,EAAA,IADA;AAEA,kBAAA,IAAA,EAAA;AAFA,iBAAA;;AAFA;AAAA;;AAAA;AAAA;AAAA,uBAOA,KAAA,CAAA;AACA,kBAAA,GAAA,EAAA,UAAA,WADA;AAEA,kBAAA,MAAA,EAAA;AAFA,iBAAA,CAAA,CAGA,IAHA,CAGA,UAAA,GAAA;AAAA,yBACA,MAAA,CAAA,KAAA,GAAA,UAAA,CAAA,GAAA,CAAA,IAAA,CADA;AAAA,iBAHA,CAPA;;AAAA;AAaA,gBAAA,OAAA,CAAA,GAAA,CAAA,MAAA,CAAA,KAAA;;AACA,oBAAA,MAAA,CAAA,KAAA,GAAA,UAAA,CAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,EAAA;AACA,kBAAA,MAAA,CAAA,QAAA,CAAA,sBAAA,EAAA,IAAA,EAAA;AACA,oBAAA,iBAAA,EAAA,IADA;AAEA,oBAAA,IAAA,EAAA;AAFA,mBAAA;AAGA,iBAJA,MAKA;AACA,kBAAA,MAAA,CAAA,aAAA,GAAA,IAAA;AACA;;AArBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAuBA,KAlFA;AAmFA,IAAA,WAnFA,yBAmFA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,QADA,GACA,IAAA,QAAA,EADA;AAEA,gBAAA,QAAA,CAAA,MAAA,CAAA,QAAA,EAAA,MAAA,CAAA,EAAA;AACA,gBAAA,QAAA,CAAA,MAAA,CAAA,UAAA,EAAA,MAAA,CAAA,QAAA;AAHA;AAAA,uBAIA,KAAA,CAAA;AACA,kBAAA,GAAA,EAAA,UAAA,cADA;AAEA,kBAAA,MAAA,EAAA,MAFA;AAGA,kBAAA,IAAA,EAAA;AAHA,iBAAA,CAAA,CAIA,IAJA,CAIA,UAAA,GAAA,EAAA;AACA,kBAAA,MAAA,CAAA,MAAA,GAAA,GAAA,CAAA,IAAA;AACA,kBAAA,OAAA,CAAA,GAAA,CAAA,MAAA,CAAA,MAAA;AACA,kBAAA,MAAA,CAAA,YAAA,GAAA,IAAA;AACA,kBAAA,MAAA,CAAA,aAAA,GAAA,KAAA;AACA,iBATA,CAJA;;AAAA;AAcA,oBAAA,MAAA,CAAA,MAAA,IAAA,GAAA,EAAA;AACA,kBAAA,MAAA,CAAA,UAAA,GAAA,sBAAA;AACA,kBAAA,MAAA,CAAA,WAAA,GAAA,MAAA;AACA,iBAHA,MAIA;AACA,kBAAA,MAAA,CAAA,UAAA,GAAA,kBAAA;AACA,kBAAA,MAAA,CAAA,WAAA,GAAA,OAAA;AACA;;AArBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAsBA;AAzGA,GAvBA;AAkIA,EAAA,OAlIA,qBAkIA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AACA;AACA,cAAA,MAAA,CAAA,WAAA,GAAA,MAAA,CAAA,UAAA;;AACA,cAAA,MAAA,CAAA,OAAA,GAHA,CAIA;;;AACA,cAAA,MAAA,CAAA,QAAA,GAAA,YAAA;AACA,gBAAA,MAAA,CAAA,WAAA,GAAA,MAAA,CAAA,UAAA;;AACA,gBAAA,MAAA,CAAA,OAAA;AACA,eAHA;;AALA;AAAA,qBASA,MAAA,CAAA,WAAA,EATA;;AAAA;AAAA;AAAA,qBAUA,MAAA,CAAA,SAAA,EAVA;;AAAA;AAWA,cAAA,MAAA,CAAA,MAAA,CAAA,MAAA,CAAA,cAAA,EAAA,MAAA,CAAA,UAAA;;AACA,kBAAA,MAAA,CAAA,KAAA,EAAA;AACA,gBAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA,QAAA;;AACA,gBAAA,MAAA,CAAA,MAAA,CAAA,MAAA,CAAA,YAAA,EAAA,KAAA;AACA;;AAfA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAgBA,GAlJA;AAmJA,EAAA,OAnJA,qBAmJA;AACA,SAAA,UAAA,GAAA,KAAA,MAAA,CAAA,KAAA,CAAA,WAAA,CAAA,UAAA;AACA;AArJA,CAAA","sourcesContent":["<template>\n  <div class=\"goods_main\">\n    <el-backtop></el-backtop>\n    <HeaderBar></HeaderBar>\n\n    <el-card id=\"goods-card\">\n      <el-row :gutter=\"20\" style=\"margin-top:30px\">\n        <el-col :span=\"9\" style=\"margin-top:53px\">\n          <div class=\"block\" style=\"margin-top:0px\">\n            <el-carousel indicator-position=\"outside\" trigger=\"click\" height=\"400px\" style=\"border-radius:0px\">\n              <el-carousel-item v-for=\"index in this.itemInfo.file.length\" :key=\"index\">\n                <img :src=\"dictionary[id+'_'+(index-1)]\" alt=\"image\">\n              </el-carousel-item>\n            </el-carousel>\n          </div>\n        </el-col>\n\n        <el-col :span=\"9\">\n          <h3 class=\"good-title\">{{itemInfo.name}}</h3>\n\n          <el-row type=\"flex\" align=\"middle\">\n            <el-col :span=\"4\">\n              <h3 class=\"good-attr\">价格: </h3>\n            </el-col>\n            <el-col :span=\"20\">\n              <h1 style=\"text-align:left;color:#FF5809;margin-top:10px;margin-bottom:10px;\">￥{{itemInfo.price}}</h1>\n            </el-col>\n          </el-row>\n\n          <el-row type=\"flex\" style=\"text-align:left;\">\n            <el-col :span=\"6\">\n              <h3 class=\"good-attr\">相关标签: </h3>\n            </el-col>\n            <el-col :span=\"18\" style=\"margin-top:10px;margin-bottom:5px;\">\n              <el-tag size=\"small\" class=\"good-tag\" v-for=\"tag in itemInfo.tag\" :key=\"tag\">{{tag}}</el-tag>\n            </el-col>\n          </el-row>\n\n          <el-row type=\"flex\">\n            <h3 class=\"good-attr\">商品描述: </h3> \n          </el-row>\n          <el-row type=\"flex\" style=\"height:150px\">\n            <p class=\"good-des\">{{itemInfo.description}}</p>\n          </el-row>\n\n          <el-row type=\"flex\" align=\"middle\" justify=\"center\">\n            <el-col :span=\"7\">\n              <el-button class=\"user-box-button\" type=\"primary\" icon=\"el-icon-s-goods\" @click=\"buyItem\" plain>购买商品</el-button>\n            </el-col>\n            <el-col :span=\"9\" :offset=\"1\">\n              <el-button class=\"user-box-button\" icon=\"el-icon-shopping-cart-2\" type=\"primary\" @click=\"addTrolley(itemInfo.id)\">加入购物车</el-button>\n            </el-col>\n          </el-row>\n        </el-col>\n\n        <el-col :span=\"5\" style=\"margin-top:53px\">\n          <el-descriptions direction=\"vertical\" :column=\"1\" border >\n            <el-descriptions-item label=\"卖家名称\">{{itemInfo.publisher}}</el-descriptions-item>\n            <el-descriptions-item label=\"QQ\">{{itemInfo.qq}}</el-descriptions-item>\n            <el-descriptions-item label=\"联系电话\">{{itemInfo.telephone}}</el-descriptions-item>\n            <el-descriptions-item label=\"邮箱\">{{itemInfo.email}}</el-descriptions-item>\n          </el-descriptions>\n\n          <el-row type=\"flex\"><el-button class=\"user-box-button\" icon=\"el-icon-chat-dot-round\" type=\"primary\" @click=\"goChatting\">与我联系</el-button></el-row>\n          <el-row type=\"flex\"><el-button class=\"user-box-button\" icon=\"el-icon-s-custom\" type=\"primary\" @click=\"goCenter\" plain>个人主页</el-button></el-row>\n        </el-col>\n      </el-row>\n    </el-card>\n\n    <el-dialog title=\"购买商品\" :visible.sync=\"dialogVisible\" width=\"50%\" center>\n      <el-row type=\"flex\">您想要购买的商品为: {{itemInfo.name}}</el-row>\n      <el-row type=\"flex\" style=\"margin-top:20px;\">价格: ￥{{itemInfo.price}}</el-row>\n      <el-row type=\"flex\" style=\"margin-top:20px;\">交易地点(请先和卖家商量好哦!):</el-row>\n      <el-input maxlength=\"150\" type=\"textarea\" autosize :rows=\"2\" placeholder=\"请输入地点\" v-model=\"location\"></el-input>\n      <el-dialog width=\"30%\" title=\"购买结果\" :visible.sync=\"innerVisible\" append-to-body>\n        <el-row type=\"flex\">{{result_txt}}</el-row>\n        <span slot=\"footer\" class=\"dialog-footer\">\n          <el-button type=\"primary\" @click=\"innerVisible = false\">{{button_text}}</el-button>\n        </span>\n      </el-dialog>\n      <span slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"dialogVisible = false\">取消购买</el-button>\n        <el-button type=\"primary\" @click=\"sendBuyInfo\">确定购买</el-button>\n      </span>\n    </el-dialog>\n\n    <FootBar></FootBar>\n  </div>\n</template>\n\n<script>\nimport HeaderBar from '../components/HeaderBar.vue'\nimport FootBar from '../components/FootBar.vue'\nimport axios from 'axios';\n    export default {\n        data() \n        {\n            return {\n                id:'',  \n                itemInfo:null,\n                screenWidth :0,\n                dictionary: {},\n                first: false,\n                money:null,\n                dialogVisible: false,\n                innerVisible: false,\n                location:'',\n                answer: null,\n                result_txt: '',\n                button_text: '',\n            };\n        },\n        \n        components: {\n          HeaderBar,\n          FootBar\n        },\n        methods:{\n          setSize() {\n            // 通过浏览器宽度(图片宽度)计算高度\n            this.bannerHeight = 400 / 1920 * this.screenWidth;\n          },\n          goPost() {\n            this.$router.push('/postInfo')\n          },\n          goCenter() {\n            this.$router.push('/center')\n          },\n          goChatting() {\n            this.$router.push('/chat')\n          },\n          async addTrolley(id) {\n            if(this.$store.state.user.user == null) {\n              this.$confirm(\"您还未登录，请先登录再将商品加入购物车\", '警告', {\n              confirmButtonText: '确定',\n              type: 'warning'})\n            }\n            else {\n              let fileList = new FormData()\n              fileList.append('itemId', id)\n              await axios({\n                url:\"/root\"+'/trolley/add',\n                method: 'post',\n                data: fileList\n              }).then(res => (console.log(res)))\n              this.$confirm('加入购物车成功! 是否前往购物车查看？', '提示', {\n                confirmButtonText: '确定',\n                cancelButtonText: '取消',\n                type: 'success'\n              }).then(() => {\n                this.$router.push('/trolley')\n              })\n            }\n          },\n          async getItemInfo() {\n            this.id = this.$store.state.SearchInfo.item_id\n            this.first = this.$store.state.image_store.reload\n            let fileList = new FormData()\n            fileList.append('itemID', this.id)\n            await axios({\n              url:\"/root\"+\"/items/detail\",\n              method:'post',\n              data:fileList\n            }).then(res => (this.itemInfo = res.data))\n          },\n          async getImages() {\n            for(var i = 0;i<this.itemInfo.file.length;i++){\n              let pic_name = this.itemInfo.file[i]\n              await axios({\n                url:\"/root\"+\"/items/picture/\"+pic_name,\n                method:\"get\"\n              }).then(res => (\n                this.dictionary[this.id+'_'+i] = res.data\n              ))\n            }\n          },\n          async buyItem() {\n            if(this.$store.state.user.user == null) {\n              this.$confirm(\"您还未登录，请先登录再购买商品\", '警告', {\n              confirmButtonText: '确定',\n              type: 'warning'})\n            }\n            else {\n              await axios({\n                url:\"/root\"+'/getmoney',\n                method: 'get'\n              }).then(res => (\n                this.money = parseFloat(res.data) \n              ))\n              console.log(this.money)\n              if(this.money < parseFloat(this.itemInfo.price)) {\n                this.$confirm(\"您的账户余额不足，请充值后再进行购买! \", '警告', {\n                  confirmButtonText: '确定',\n                  type: 'warning'})\n              }\n              else {\n                this.dialogVisible = true\n              }\n            }\n          },\n          async sendBuyInfo() {\n            let fileList =  new FormData()\n            fileList.append('itemID', this.id)\n            fileList.append('location', this.location)\n            await axios({\n                url:\"/root\"+'/trolley/buy',\n                method: 'post',\n                data: fileList\n            }).then(res => {\n              this.answer = res.data\n              console.log(this.answer)\n              this.innerVisible = true\n              this.dialogVisible = false\n            })\n            if (this.answer == \"1\"){\n              this.result_txt = \"购买成功!一封确认邮件已经发至您的邮箱!\"\n              this.button_text = \"太棒了!\"\n            }\n            else {\n              this.result_txt = \"购买失败，好像出了点小问题...\"\n              this.button_text = \"啊这...\"\n            }\n          }\n        },\n        async created() {\n          // 首次加载时,需要调用一次\n          this.screenWidth =  window.innerWidth;\n          this.setSize();\n          // 窗口大小发生改变时,调用一次\n          window.onresize = () =>{\n            this.screenWidth =  window.innerWidth;\n            this.setSize();\n          }\n          await this.getItemInfo()\n          await this.getImages()\n          this.$store.commit('SET_ADD_ITEM', this.dictionary)\n          if(this.first){\n            this.$router.push('/blank')\n            this.$store.commit('SET_RELOAD', false)\n          }  \n        },\n        updated() {\n          this.dictionary = this.$store.state.image_store.item_image\n        }\n    }\n</script>\n\n<style>\n  .el-carousel__item h3 {\n    color: #475669;\n    font-size: 14px;\n    opacity: 0.75;\n    line-height: 150px;\n    margin: 0;\n  }\n  .user-box-button {\n    width: 100%;\n    margin-top: 10px;\n  }\n  img {\n      width:100%;\n      height:inherit;\n    }\n  #goods-card {\n    width: 75%;\n    border-radius: 15px;\n    margin: auto;\n    margin-top:20px;\n    height: 600px;\n  }\n  .good-title {\n    margin-top:0px;\n    margin-bottom: 5px;\n    font-size:20px;\n    text-align:left;\n  }\n  .good-tag {\n    margin-right:5px;\n    text-align:center;\n    margin-bottom:5px;\n  }\n  .good-attr {\n    text-align:left;\n    margin-top:10px;\n    margin-bottom:10px;\n  }\n  .good-des {\n    text-align:left;\n    margin-top:10px;\n    font-size: 16px;\n    color: darkgray;\n    margin-bottom: 10px;\n  }\n  \n</style>\n"],"sourceRoot":"src/views"}]}